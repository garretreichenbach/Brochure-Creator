{
  "name": "brochure-creator",
  "version": "1.0.0",
  "description": "Create customized brochures for locations in Super Mario Odyssey style using Google Gemini",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run start-all",
    "start-all": "npm-run-all --parallel start-react start-express",
    "start-react": "cross-env PORT=3004 react-scripts start",
    "start-express": "nodemon --config nodemon.json server/index.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.0",
    "@googlemaps/js-api-loader": "^1.16.2",
    "@react-google-maps/api": "^2.20.6",
    "axios": "^1.8.4",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "markdown-it": "^14.0.0",
    "pdfkit": "^0.14.0",
    "puppeteer": "^22.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "react-scripts": "5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
